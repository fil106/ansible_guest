server {

	listen 80;
 
    access_log   /var/log/nginx/guestbook.log;
    error_log    /var/log/nginx/guestbook.log;

	root /opt/app/front;

	index index.html;

    location = /notes {
      proxy_pass http://127.0.0.1:5000;
    }
    location ^~ /comment/ {
      proxy_pass http://127.0.0.1:5000;
    }
    location ^~ /note {
      proxy_pass http://127.0.0.1:5000;
    }
}